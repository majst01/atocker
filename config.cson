"*":
  welcome:
    showOnStartup: false
  core:
    themes: [
      "atom-material-ui"
      "atom-material-syntax"
    ]
    ignoredNames: [
      ".atocker"
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      ".Trash-*"
    ]
  editor:
    invisibles: {}
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: false
  minimap:
    absoluteMode: true
  "symbols-tree-view":
    autoToggle: true
  "go-plus": {}

