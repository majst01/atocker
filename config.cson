"*":
  welcome:
    showOnStartup: false
  "atom-material-ui":
    fonts:
      useRobotoInUI: true
    panels:
      depth: true
      panelContrast: true
    tabs:
      rippleAccentColor: true
    treeView:
      compactTreeView: true
    ui: {}
  core:
    themes: [
      "atom-material-ui"
      "atom-material-syntax-light"
    ]
    ignoredNames: [
      ".atocker"
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      ".Trash-*"
    ]
  editor:
    invisibles: {}
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: false
  minimap:
    absoluteMode: true
    plugins:
      bookmarks: true
      "find-and-replace": true
  "symbols-tree-view":
    autoToggle: true
  "go-plus": {}

